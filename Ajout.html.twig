
{% extends 'base-back.html.twig' %}
{% block body %}

    <div class="beinvenue_div">
        <img src="{{ asset('back-office/assets/images/hair.png' )}}"  alt="welcome"
             style="width:1200px;height:200px;">

    </div>
    {# {{ form(f) }} #}

    {{ form_start(f,{'attr':{'novalidate':'novalidate'}})}}


    <div class="col-md-2" style=" color: #0b5b97; font-size: large; font-weight: bold">{{ form_row(f.prixSEsth) }}</div>

    <div class="col-md-2" style=" color: #0b5b97; font-size: large; font-weight: bold">{{ form_row(f.nolSEsth) }}</div>

    <div class="col-sm-3 offset-md-0" >
        <button type="submit" class="btn btn-sm btn-light btn-block"style=" color: #0b5b97; font-size: large; font-weight: bold">Ajouter</button>
    </div>
    {{ form(f,{'method':'GET'}) }}
    {{ form_end(f) }}
{% endblock %}