<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AffichageController">
   <children>
      <ImageView fx:id="imageView" fitHeight="400.0" fitWidth="602.0" layoutY="1.0">
         <image>
            <Image url="@../image/R.jpg" />
         </image>
      </ImageView>
      <AnchorPane fx:id="pane" layoutX="-1.0" layoutY="4.0" prefHeight="21.0" prefWidth="30.0" />
      <Pane layoutX="55.0" layoutY="14.0" prefHeight="72.0" prefWidth="470.0" style="-fx-background-color: rgba(0,0,0,.5);">
         <children>
            <Label alignment="CENTER" layoutX="79.0" layoutY="5.0" prefHeight="60.0" prefWidth="289.0" text="Welcome " textAlignment="JUSTIFY" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="System Bold Italic" size="39.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TableView fx:id="tableProduit" layoutX="55.0" layoutY="116.0" prefHeight="200.0" prefWidth="470.0">
        <columns>
          <TableColumn fx:id="colid" prefWidth="78.0" text="ID" />
          <TableColumn fx:id="colnom" prefWidth="116.0" text="Nom" />
            <TableColumn fx:id="colprix" minWidth="7.0" prefWidth="128.0" text="Prix" />
            <TableColumn fx:id="colcategorie" prefWidth="147.0" text="Categorie" />
        </columns>
      </TableView>
      <Pane layoutX="54.0" layoutY="327.0" prefHeight="72.0" prefWidth="470.0" style="-fx-background-color: rgba(0,0,0,.5);">
         <children>
            <Button fx:id="btnajouter" layoutX="26.0" layoutY="2.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="25.0" prefWidth="79.0" style="-fx-background-color: #9ACD32;" text="Ajouter" />
            <Button fx:id="btnmodifier" layoutX="196.0" layoutY="2.0" mnemonicParsing="false" onAction="#modifier" prefHeight="25.0" prefWidth="79.0" style="-fx-background-color: #9ACD32;" text="Modifier" />
            <Button fx:id="btnsupprimer" layoutX="333.0" layoutY="2.0" mnemonicParsing="false" onAction="#supprimer" prefHeight="25.0" prefWidth="93.0" style="-fx-background-color: #9ACD32;" text="Supprimer" />
            <Button fx:id="buttonPDF" layoutX="106.0" layoutY="45.0" mnemonicParsing="false" onAction="#PDF" prefHeight="25.0" prefWidth="263.0" style="-fx-background-color: #9ACD32;" text="PDF" />
         </children>
      </Pane>
   </children>
</AnchorPane>
